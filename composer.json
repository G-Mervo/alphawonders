{
    "name": "alphawonders/website",
    "description": "Alphawonders Solutions Website",
    "type": "project",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=7.4",
        "codeigniter4/framework": "^4.4",
        "monolog/monolog": "^1.27|^2.0|^3.0",
        "mpdf/mpdf": "^8.0",
        "facebook/graph-sdk": "^5.7|^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5|^9.5",
        "fakerphp/faker": "^1.9"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "scripts": {
        "post-update-cmd": [
            "@php spark migrate"
        ]
    },
    "support": {
        "source": "https://github.com/codeigniter4/CodeIgniter4",
        "discussions": "https://forum.codeigniter.com/",
        "issues": "https://github.com/codeigniter4/CodeIgniter4/issues"
    }
}
